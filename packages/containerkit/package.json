{
  "name": "containerkit",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "#*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./dist/containerkit.css"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "format": "prettier . --write",
    "test": "pnpm -w test --project=containerkit",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@webcontainer/api": "^1.6.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "monaco-editor": "^0.54.0"
  },
  "devDependencies": {
    "@containerkit/linter-config": "workspace:*",
    "@containerkit/typescript-config": "workspace:*",
    "rollup-plugin-copy": "^3.5.0",
    "tsdown": "catalog:build",
    "typescript": "catalog:build"
  }
}
